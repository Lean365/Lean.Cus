# 应用名称
VITE_APP_TITLE=Lean.Cus

# API 基础路径
VITE_API_BASE_URL=/api

# 上传文件路径
VITE_UPLOAD_URL=/api/upload

# WebSocket 路径
VITE_WEBSOCKET_URL=/signalr/hub

# 是否启用加密
VITE_ENABLE_ENCRYPTION=true

# 是否启用 Mock 数据
VITE_USE_MOCK=false

# 是否启用压缩
VITE_BUILD_COMPRESS=true

# 是否删除控制台日志
VITE_DROP_CONSOLE=true

# 是否启用 PWA
VITE_PWA=false

# 是否启用主题切换
VITE_THEME_SWITCH=true

# 默认主题
VITE_DEFAULT_THEME=light

# 默认语言
VITE_DEFAULT_LOCALE=zh-CN

# 是否启用权限控制
VITE_USE_PERMISSION=true

# 是否启用水印
VITE_USE_WATERMARK=true

# 是否启用全屏
VITE_USE_FULLSCREEN=true

# 是否启用标签页
VITE_USE_TABS=true

# 是否启用面包屑
VITE_USE_BREADCRUMB=true

# 是否启用返回顶部
VITE_USE_BACK_TOP=true

# 是否启用主题色
VITE_USE_THEME_COLOR=true

VITE_API_URL=http://localhost:5172 